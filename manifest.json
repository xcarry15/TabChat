{
  "manifest_version": 3,
  "name": "TabChat",
  "short_name": "TabChat",
  "version": "0.3.0",
  "description": "覆盖新标签页：时间+输入；Enter 搜索，Ctrl+Enter AI（硅基流动）。支持流式、对话记忆、Markdown、设置。",
  "minimum_chrome_version": "114",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": [
    "storage",
    "search"
  ],
  "host_permissions": [
    "https://api.siliconflow.cn/*",
    "https://*.siliconflow.cn/*"
  ],
  "icons": {
    "16": "icons/icon-16.png",
    "32": "icons/icon-32.png",
    "48": "icons/icon-48.png",
    "128": "icons/icon-128.png"
  }
}

